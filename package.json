{
  "name": "aqora",
  "displayName": "aqora",
  "description": "aqora cli wrapped for vscode",
  "version": "0.3.0",
  "publisher": "aqora-quantum",
  "repository": "https://github.com/aqora-io/vscode-aqora",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Snippets",
    "Notebooks",
    "Other"
  ],
  "icon": "images/icon_white.png",
  "bugs": {
    "url": "https://github.com/aqora-io/vscode-aqora/issues"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aqora.templateCompetition",
        "title": "Aqora: Clone a template from aqora.io."
      },
      {
        "command": "aqora.login",
        "title": "Aqora: Login into aqora platform by using CLI."
      },
      {
        "command": "aqora.test",
        "title": "Aqora: Test your submission"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
    "watch": "pnpm watch:codegen && pnpm watch:tsc && pnpm watch:esbuild",
    "watch:codegen": "graphql-codegen -w --config ./src/codegen.ts",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "codegen": "graphql-codegen --config ./src/codegen.ts"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.12.13",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@parcel/watcher": "^2.4.1",
    "@types/luxon": "^3.4.2",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "esbuild": "^0.21.5",
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "apollo-cache": "^1.3.5",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "cross-fetch": "^4.0.0",
    "graphql": "^15.9.0",
    "graphql-http": "^1.22.1",
    "luxon": "^3.5.0",
    "smol-toml": "^1.3.0",
    "xdg-app-paths": "^8.3.0"
  },
  "packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1"
}
